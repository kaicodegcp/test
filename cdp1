vi /docker2/load_tag_push_images.sh
ðŸ“œ Paste this inside:
bash
Copy
Edit
#!/bin/bash

IMAGE_DIR="/docker2/1.5.4-h10/images"
REGISTRY="sd-wv58-kflz.nam.nsroot.net:5000"
LOG_FILE="/docker2/image_load_push.log"

echo "ðŸ”„ Starting image load & push at $(date)" > $LOG_FILE
cd $IMAGE_DIR || exit 1

for file in *.tar.gz; do
  echo "ðŸ“¦ Loading $file..." | tee -a $LOG_FILE
  docker load < "$file" >> $LOG_FILE 2>&1
done

echo "âœ… All images loaded. Tagging now..." | tee -a $LOG_FILE

docker images | grep -v "<none>" | grep -v "$REGISTRY" | while read repo tag id rest; do
  new_tag="$REGISTRY/$repo:$tag"
  echo "ðŸ”– Tagging $repo:$tag -> $new_tag" | tee -a $LOG_FILE
  docker tag "$repo:$tag" "$new_tag"
done

echo "ðŸš€ Pushing images to $REGISTRY..." | tee -a $LOG_FILE

docker images | grep "$REGISTRY" | while read repo tag id rest; do
  echo "â¬†ï¸  Pushing $repo:$tag..." | tee -a $LOG_FILE
  docker push "$repo:$tag" >> $LOG_FILE 2>&1
done

echo "ðŸŽ‰ Done! Finished at $(date)" | tee -a $LOG_FILE
